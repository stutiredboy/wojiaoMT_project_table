		<!-- 客户端请求访问 NPC -->
	<protocol name="CVisitNpc" type="9001" maxsize="65535" prior="1">
			<variable name="npckey" type="long" validator="value=[0,)" />  npckey为npc的唯一ID
	</protocol>
	
	<protocol name="SVisitNpc" type="9002" maxsize="65535" prior="1" tolua="3">
			<variable name="npckey" type="long" validator="value=[0,)" /> npckey为npc的唯一ID
			<variable name="services" type="vector" value="int" /> services 为npc所提供的服务 IDs.
			<variable name="scenarioquests" type="vector" value="int"/>剧情任务
	</protocol>
	
	<protocol name="CNpcService" type="9003" maxsize="65535" prior="1" tolua="3">		通用向服务器发送NPC服务消息（不带参数）
			<variable name="npckey" type="long" />	npckey为npc的唯一ID
			<variable name="serviceid" type="int" />						服务ID
	</protocol>
	
		<!-- 向NPC请求问题 -->
	<protocol name="CReqQuestion" type="9007" maxsize="65535" prior="1" tolua="3">
			<variable name="npckey" type="long" validator="value=[0,)" /> npckey为npc的唯一ID
	</protocol>
	
			<!-- 向NPC请求问题的答案 -->
	<protocol name="CAnsQuestion" type="9009" maxsize="65535" prior="1" tolua="3">
			<variable name="npckey" type="long" validator="value=[0,)" /> npckey为npc的唯一ID
			<variable name="questionid" type="int" validator="value=[0,)" />
		  <variable name="answer" type="int" validator="value=[0,)" />玩家的回答
	</protocol>
	

	<!-- 服务器发送抽奖 -->
	<protocol name="SReqFortuneWheel" type="9013" maxsize="65535" prior="1" tolua="3">
			<variable name="npckey" type="long" validator="value=[0,)" /> npckey为npc的唯一ID
			<variable name="serviceid" type="int"/> 						当前转盘服务id
			<variable name="itemids" type="vector" value="ForturneWheelType" /> 供选择的12个物品,可能是钱或经验
			<variable name="index" type="int" validator="value=[0,)" /> 最后获得的物品下标
			<variable name="flag" type="byte"/> 0 大转盘  1翻盘子
	</protocol>
	
	<!-- 客户端请求领奖 -->
	<protocol name="CFinishFortuneWheel" type="9014" maxsize="65535" prior="1" tolua = "3">
			<variable name="npckey" type="long" validator="value=[0,)" />	npckey为npc的唯一ID
			<variable name="serviceid" type="int"/> 							当前转盘服务id
	</protocol>
		
	<protocol name="SBattleToNpcError" type="9021" maxsize="65535" prior="1" tolua = "3">
		<variable name="battleerror" type="int" validator="value=(,0)" /> 错误代号
	</protocol>
	
	<!-- 服务器发送给客户端，npc提示信息-->
	<protocol name="SSendNpcMsg" type="9022" maxsize="65535" prior="1" tolua = "3">	
		<variable name="npckey" type="long" validator="value=(0,)" />
		<variable name="npcid" type="int" validator="value=(0,)" />
		<variable name="msgid" type="int" validator="value=(0,)" /> 
		<variable name="args" type="vector" value="long" /> 
	</protocol>
	
	<!-- 通知客户端提交任务物品或者任务宠物-->
	<protocol name="SSubmit2Npc" type="9023" maxsize="65535" prior="1" tolua="3">
		<variable name="questid" type="int" validator="value=[0,)" /> 提交的相关的任务或者其他相关id
		<variable name="npckey" type="long" validator="value=(0,)" /> 提交的npc
		<variable name="submittype" type="int" validator=" value=[0,)"  /> 提交类型: 道具0 或者宠物1
		<variable name="availableIds" type="vector" value="int"  validator="value=[0,)" /> 可以提交的道具ID 或者宠物ID
		<variable name="availablePos" type="int"  validator="value=[0,5]"/> 开放提交的格子数量
	</protocol>
	
	<bean name="SubmitUnit">
		<variable name="key" type="int" validator="value=(0,)" /> 给予物品或者道具的key
		<variable name="num" type="int" validator="value=(0,)" /> 给予物品或者道具的数量
	</bean>
	
	<!-- 客户端请求提交任务物品或者任务宠物-->
	<protocol name="CSubmit2Npc" type="9024" maxsize="65535" prior="1" tolua="3">
		<variable name="questid" type="int" validator="value=[0,)" /> 提交物品相关的任务或者其他相关id
		<variable name="npckey" type="long" validator="value=[0,)" /> npckey
		<variable name="submittype" type="int" validator="value=[0,)" /> 提交的类型
		<variable name="things" type="vector" value="SubmitUnit" /> 提交的道具key 或者宠物的key
	</protocol>
				<!-- 玩家完成转盘,发给玩家奖励 -->
	<protocol name="CDoneFortuneWheel" type="9025" maxsize="65535" prior="1" tolua="3">
		<variable name="npckey" type="long" validator="value=[0,)" />	npckey为npc的唯一ID
		<variable name="taskid" type="int"/> 							当前转盘的任务id
		<variable name="succ" type="int"/> 	1表示正常完成,0表示玩家提前关闭了大转盘
		<variable name="flag" type="byte"/> 0 大转盘  1翻盘子
	</protocol>	
	
