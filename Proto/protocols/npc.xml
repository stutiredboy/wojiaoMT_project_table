		<variable name="accuexp" type="int" />			本次累积获得多少经验
		<variable name="accumoney" type="int" />		本次累积获得多少钱
		<variable name="delwrongval" type="int" />		地王令
		<variable name="chorightval" type="int" />		天王令
		<variable name="helpcnt" type="int" />			求助次数
	</bean>	 
	
