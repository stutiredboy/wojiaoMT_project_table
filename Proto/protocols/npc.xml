	<!-- 答题统一协议 -->
	<protocol name="SAskQuestion" type="9088" maxsize="1024" prior="1" tolua="3"> 服务器出题
		<variable name="lastresult" type="byte"/> 0 = 没有上一道题，1 = 正确，-1 = 错误
		<variable name="questionid" type="int"/> 下一道题ID，如果没有下一道为-1
		<variable name="questiontype" type="int"/> 答题类型，根据不同的类型，客户端/服务器可能会有不同的处理
		<variable name="npckey" type="long"/> npckey
		<variable name="xiangguanid" type="int"/>可能需要的相关id
        <variable name="lasttime" type="int"/> 该题剩余时间(秒)
		<variable name="cur" type="int"/> 当前答对几道 by changhao
		<variable name="num" type="int"/> 已经答了多少道 by changhao
		<variable name="totalexp" type="int"/>  给的总经验 by changhao
		<variable name="totalmoney" type="int"/> 给的总金钱 by changhao
		<variable name="helptimes" type="int"/> 已经求助的次数 by changhao
		<variable name="grab" type="int" /> 1是可以领取2是已经领取3是不能领取 by changhao
		
		<variable name="rightanswer" type="list" value="int"/> 已经回答过的多选正确答案

        <enum name="QUEST" value="1" /> 任务
		<enum name="INSTANCE_ZONE" value="2" /> 副本玩法
		<enum name="FRIEND_NPC_CHAT" value="3" /> 与结交的npc聊天答题
		<enum name="SPECIALQUEST_ANSWER" value="5" /> 职业答题分支任务
        <enum name="GUILD_ANSWER" value="7" /> 公会副本任务
		<enum name="ACTIVITY_ANSWER" value="8"/> 活动答题 by changhao
    </protocol>
	
	<protocol name="CAnswerQuestion" type="9089" maxsize="1024" prior="1" tolua="3"> 客户端答题
		<variable name="questionid" type="int"/>
		<variable name="answerid" type="int"/> [1,) 最多几个答案选项？
		<variable name="questiontype" type="int"/> 答题类型，根据不同的类型，客户端/服务器可能会有不同的处理
		<variable name="npckey" type="long"/>
		<variable name="xiangguanid" type="int"/>可能需要的相关id
	</protocol>

